Xcursor.theme: Neutral
Xcursor.size: 16

URxvt.preeditType:      OverTheSpot
URxvt.jumpScroll:       true
URxvt.skipScroll:       true

URxvt.urgentOnBell:     true 

URxvt*font:             xft:Terminus-10,xft:mplus_j10r:antialias=false,xft:Sazanami Gothic:antialias=false,xft:Code2000:antialias=false
