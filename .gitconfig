[alias]
  ci = commit -a
  co = checkout
  st = status
  pum = pull origin master
  
[color]
  ui = auto

[user]
  name = muflax
  email = mail@muflax.com

[push]
  default = matching
  
[github]
  user = muflax

[core]
	excludesfile = .gitignore-global

[include]
   path = .git-local
[merge]
	tool = /usr/bin/meld
[gui]
	encoding = utf-8
